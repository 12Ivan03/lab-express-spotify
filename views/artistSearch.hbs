<h1>search result:</h1>


    {{#each artists}}
        <div class="artist-info">
            <h2>{{name}}</h2>
            {{#if images.length}}
            <img src="{{images.[0].url}}" alt="{{name}} Image" />
            {{else}}
            <p>No image available</p>
            {{/if}}
           
           {{!-- <a href="/albums/{{id}}">View Albums</a> --}}
        </div>
    {{/each}}
